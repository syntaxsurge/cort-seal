{
  "name": "cort-seal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:code": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,cts,mts,json,css,scss,mdx,html}\"",
    "lint:all": "pnpm run lint:fix && pnpm run lint && pnpm run format:code",
    "typecheck": "node -e \"require('fs').rmSync('.next/types',{recursive:true,force:true})\" && tsc --noEmit",
    "convex:dev": "node ./scripts/convex-dev.cjs",
    "convex:deploy": "npx convex deploy -y",
    "convex:redeploy": "pnpm run convex:deploy && pnpm run convex:dev",
    "convex:reset": "dotenv -e .env -e .env.local -- tsx scripts/reset-convex.ts",
    "b2:set-cors": "dotenv -e .env -e .env.local -- tsx scripts/b2-set-cors.ts",
    "convex:codegen": "convex codegen"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.31.0",
    "lucide-react": "^0.561.0",
    "next": "15.5.9",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "rss-parser": "^3.13.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.9",
    "prettier": "^3.2.5",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
